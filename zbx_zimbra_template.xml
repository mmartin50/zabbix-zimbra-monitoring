                "name": "MyTemplate App Zimbra",
                "groups": [
                    {
                        "name": "Templates"
                    }
                ],
                "discovery_rules": [
                    {
                        "uuid": "687786c8a2c6435bb0f32f094b121402",
                        "name": "Zimbra service discovery",
                        "key": "zimbra.discovery",
                        "delay": "3600",
                        "item_prototypes": [
                            {
                                "uuid": "c5a8956016274e6386979de3c9e0b168",
                                "name": "ZIMBRASERVICE status",
                                "key": "zimbra.status.[{#ZIMBRASERVICE}]",
                                "delay": "120",
                                "valuemap": {
                                    "name": "Service state"
                                },
                                "tags": [
                                    {
                                        "tag": "Application",
                                        "value": "Zimbra service status"
                                    }
                                ],
                                "trigger_prototypes": [
                                    {
                                        "uuid": "638c7586177640bba06359271c65f7f1",
                                        "expression": "last(/MyTemplate App Zimbra/zimbra.status.[{#ZIMBRASERVICE}])=0",
                                        "name": "Zimbra service ({#ZIMBRASERVICE}) is not running on {HOSTNAME}"
                                    }
                                ]
                            }
                        ]
                    }
                ],
                "valuemaps": [
                    {
                        "uuid": "6d01b62b928e4b559c6dcfcb0e181edf",
                        "name": "Service state",
                        "mappings": [
                            {
                                "value": "0",
                                "newvalue": "Down"
                            },
                            {
                                "value": "1",
                                "newvalue": "Up"
                            }
                        ]
                    }
                ]
            }
        ]
    }
